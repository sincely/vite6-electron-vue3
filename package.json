{
  "name": "vite6-electron-vue3",
  "version": "0.0.8",
  "type": "module",
  "main": "dist-electron/main/index.js",
  "engines": {
    "node": ">=16.14"
  },
  "description": "A Vite6 Electron Vue3 project",
  "author": "chengzhou",
  "scripts": {
    "dev": "chcp 65001 && vite",
    "dev:mac": "vite --mode development",
    "electron:postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build:win:prod",
    "build:mac": "npm run build:mac:prod",
    "build-win:test": "vite build --mode test && electron-builder --win",
    "build-mac:test": "vite build --mode test && electron-builder --mac",
    "build-win:prod": "vite build --mode production && electron-builder --win",
    "build-mac:prod": "vite build --mode production && electron-builder --mac",
    "build:test": "npm run build-win:test && npm run build-mac:test",
    "build:production": "npm run build-win:prod && npm run build-mac:prod",
    "preview": "vite preview --port 4173",
    "lint": "npm run eslint:fix && npm run prettier:fix && npm run stylelint:fix",
    "eslint:fix": "eslint \"**/*.{vue,js}\" --fix",
    "prettier:fix": "prettier --write \"**/*.{js,vue,scss,css,json,html}\"",
    "stylelint:fix": "stylelint \"src/**/*.{vue,css,scss,css}\" --fix",
    "bump:patch": "changelogen --bump --output CHANGELOG.md --release --prerelease",
    "bump:minor": "changelogen --bump --output CHANGELOG.md --release --minor",
    "bump:major": "changelogen --bump --output CHANGELOG.md --release --major",
    "manual:release": "node ./scripts/release.js",
    "release": "release-it",
    "release:dry-run": "release-it --dry-run",
    "release:changelog": "release-it --only-changelog",
    "release:github": "release-it --ci --github.release",
    "release:patch": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "release:alpha": "release-it --preRelease=alpha",
    "release:beta": "release-it --preRelease=beta",
    "dir-tree": "esno ./scripts/dir-tree",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.3.2",
    "axios": "1.6.8",
    "dayjs": "1.11.10",
    "element-plus": "2.9.11",
    "js-md5": "0.7.3",
    "lodash-es": "4.17.21",
    "md5": "2.3.0",
    "mockjs": "1.1.0",
    "nprogress": "0.2.0",
    "pinia": "2.3.1",
    "pinia-plugin-persistedstate": "3.2.3",
    "terser": "5.24.0",
    "unplugin-auto-import": "19.3.0",
    "unplugin-vue-components": "28.8.0",
    "vite-plugin-html": "3.2.2",
    "vite-plugin-style-import": "2.0.0",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-svg-loader": "4.0.0",
    "vue": "3.5.29",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.1",
    "@eslint/js": "8.57.0",
    "@iconify-json/lucide": "^1.2.94",
    "@release-it/conventional-changelog": "10.0.4",
    "@vitejs/plugin-legacy": "6.0.2",
    "@vitejs/plugin-vue": "5.2.3",
    "autoprefixer": "10.4.14",
    "changelogen": "0.5.5",
    "conventional-changelog-angular": "8.1.0",
    "conventional-changelog-conventionalcommits": "9.1.0",
    "directory-tree": "3.5.1",
    "electron": "28.3.3",
    "electron-builder": "24.13.3",
    "electron-log": "5.4.3",
    "electron-updater": "6.3.9",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-define-config": "2.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "9.18.1",
    "esno": "4.7.0",
    "terser": "5.24.0",
    "fs-extra": "10.0.0",
    "husky": "8.0.3",
    "inquirer": "13.2.2",
    "lint-staged": "13.3.0",
    "postcss": "8.4.33",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.2.5",
    "release-it": "19.2.4",
    "sass": "1.90.0",
    "stylelint": "16.8.2",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-recess-order": "4.6.0",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-order": "6.0.4",
    "stylelint-scss": "6.11.0",
    "treeify": "1.1.0",
    "vite": "6.3.6",
    "vite-plugin-electron": "0.28.8",
    "vite-plugin-electron-renderer": "0.14.6",
    "vite-plugin-inspect": "11.0.0",
    "vite-plugin-restart": "0.4.2",
    "vite-plugin-vue-devtools": "7.7.2",
    "vue-eslint-parser": "9.4.2"
  }
}
